# tests/test_round9_features.fl

print("--- Testing List Modification (Returning New Lists) ---")
let l1 = [1, 2, 3]
let l2 = append(l1, 4)
print("l1: ")
print(l1)
print("append(l1, 4): ")
print(l2)

let l3 = [5, 6]
let l4 = extend(l1, l3)
print("extend(l1, l3): ")
print(l4)

let l5 = insert(l1, 1, 99)
print("insert(l1, 1, 99): ")
print(l5)

print("\n--- Testing String Extras ---")
let s1 = "flux LANGUAGE"
print("capitalize('flux LANGUAGE'): ")
print(capitalize(s1))

let s2 = "   "
print("isspace('   '): ")
print(isspace(s2))
print("isspace('flux'): ")
print(isspace("flux"))
print("isspace(''): ")
print(isspace(""))

print("\n--- Testing Math Extras ---")
print("radians(180): ")
print(radians(180)) # Should be pi
print("degrees(pi): ")
print(degrees(pi))   # Should be 180

print("\n--- Combined Test ---")
let cmds = ["  ", "hello", "WORLD"]
let result = []
for c in cmds:
    if not isspace(c):
        let result = append(result, capitalize(c))
print("Processed commands: ")
print(result)
