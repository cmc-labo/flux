print("--- Testing Randomness ---")
let r = random()
print("random(): ")
print(r)
if r >= 0.0 && r < 1.0:
    print("random() is in range [0, 1)")

let ri = randint(1, 10)
print("randint(1, 10): ")
print(ri)
if ri >= 1 && ri <= 10:
    print("randint is in range [1, 10]")

print("\n--- Testing List Conversion ---")
let s = "Flux"
let l = list(s)
print("list('Flux'): ")
print(l)
print("type(list('Flux')): ")
print(type(l))

