print("--- Testing Trigonometry ---")
print("sin(pi/2): ")
print(sin(pi / 2))
print("cos(pi): ")
print(cos(pi))
print("tan(pi/4): ")
print(tan(pi / 4))

print("\n--- Testing String Property Checks ---")
let s1 = "12345"
let s2 = "Flux"
let s3 = "Flux123"

print("isdigit('12345'): ")
print(isdigit(s1))
print("isdigit('Flux'): ")
print(isdigit(s2))

print("isalpha('Flux'): ")
print(isalpha(s2))
print("isalpha('Flux123'): ")
print(isalpha(s3))

print("\n--- Testing List Utilities ---")
let l = [1, 2, 3, 1, 4, 1, 5]
print("List: ")
print(l)
print("count(l, 1): ")
print(count(l, 1))
print("count(l, 9): ")
print(count(l, 9))

print("\n--- Combined Test ---")
let data = ["10", "abc", "20", "def", "30"]
let sum_val = 0
for d in data:
    if isdigit(d):
        let sum_val = sum_val + int(d)
print("Sum of digits in list: ")
print(sum_val)
